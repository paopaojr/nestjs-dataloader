{
  "name": "nestjs-dataloader",
  "version": "2.0.6",
  "description": "A NestJS decorator for dataloader",
  "license": "MIT",
  "repository": "https://github.com/krislefeber/nestjs-dataloader",
  "author": "Kris Lefeber <krislefeber@gmail.com>",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "prebuild": "rm -rf ./dist",
    "prepare": "tsc -p tsconfig.json",
    "prestart": "rm -rf ./example/dist",
    "start": "nest start example/src/main.ts --watch --path=example/tsconfig.json",
    "test": "jest"
  },
  "keywords": [
    "nestjs",
    "dataloader",
    "graphql"
  ],
  "dependencies": {
    "@nestjs/apollo": "^12.0.11",
    "dataloader": "^2.2.2",
    "rxjs": "^7.5.6"
  },
  "devDependencies": {
    "@apollo/server": "4.10.0",
    "@nestjs/cli": "^10.3.0",
    "@nestjs/common": "^10.3.0",
    "@nestjs/core": "^10.3.0",
    "@nestjs/graphql": "^12.0.11",
    "@nestjs/platform-express": "^10.3.0",
    "@nestjs/testing": "^10.3.0",
    "@nestjs/typeorm": "^10.0.1",
    "@types/jest": "28.1.4",
    "graphql": "^16.8.1",
    "jest": "28.1.3",
    "reflect-metadata": "^0.1.12",
    "sqlite3": "^5.0.11",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.8",
    "typeorm": "0.2.18",
    "typeorm-factory": "^0.0.14",
    "typescript": "4.7.4"
  },
  "types": "index.d.ts",
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "collectCoverage": true,
    "globals": {
      "ts-jest": {
        "diagnostics": {
          "warnOnly": true
        }
      }
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.ts$",
    "collectCoverageFrom": [
      "index.ts"
    ]
  },
  "files": [
    "dist"
  ]
}
